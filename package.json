{
  "name": "ember-c3",
  "version": "3.0.2",
  "description": "Ember addon library for C3, a D3-based reusable chart library.",
  "keywords": [
    "ember-addon",
    "ember-cli",
    "ember-c3",
    "c3",
    "d3",
    "chart",
    "graph"
  ],
  "repository": "https://github.com/Glavin001/ember-c3",
  "license": "MIT",
  "author": {
    "name": "Glavin Wiechert",
    "email": "glavin.wiechert@gmail.com",
    "url": "https://github.com/Glavin001"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "maxwondercorn <> (https://github.com/maxwondercorn)"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "http://glavin001.github.io/ember-c3/",
    "configPath": "tests/dummy/config"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog -u --output CHANGELOG.md --commit-limit false",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "release": "standard-version  && git push --follow-tags"
  },
  "dependencies": {
    "@ember/render-modifiers": "2.0.4",
    "broccoli-funnel": "3.0.8",
    "broccoli-merge-trees": "4.2.0",
    "c3": "0.7.20",
    "d3": "6.6.0",
    "ember-auto-import": "2.4.2",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.0.1",
    "ember-concurrency": "2.2.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@embroider/test-setup": "^1.8.0",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "4.6.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "1.11.3",
    "ember-cli": "~3.16.2",
    "ember-cli-bootstrap-4": "0.13.0",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^2.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "2.4.1",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "popper.js": "^1.16.1",
    "qunit-dom": "^2.0.0",
    "sass": "1.52.3",
    "standard-version": "9.5.0",
    "webpack": "^5.73.0"
  },
  "engines": {
    "node": ">= 14"
  }
}
